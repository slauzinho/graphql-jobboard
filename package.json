{
  "name": "hackernews",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.4.0",
    "date-fns": "^1.30.1",
    "formik": "^1.5.7",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.0.8",
    "react-scripts": "3.0.1",
    "react-spring": "^8.0.22",
    "styled-components": "^4.2.0",
    "styled-normalize": "^8.0.6",
    "typescript": "3.4.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "schema:download": "apollo schema:download --endpoint=https://api.graphql.jobs/",
    "codegen:generate": "apollo codegen:generate --queries=./src/**/*.tsx --localSchemaFile=./schema.json --outputFlat --target=typescript ./src/schemaTypes.ts",
    "gen:types": "npm run schema:download && npm run codegen:generate",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@reach/router": "^1.2.1",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "24.0.13",
    "@types/node": "12.0.2",
    "@types/reach__router": "^1.2.4",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4"
  },
  "resolutions": {
    "graphql": "14.3.1"
  }
}
